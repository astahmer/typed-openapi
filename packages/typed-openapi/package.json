{
  "name": "typed-openapi",
  "type": "module",
  "version": "2.1.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.export.js",
    "./pretty": "./dist/pretty.export.js"
  },
  "bin": {
    "typed-openapi": "bin.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astahmer/typed-openapi.git",
    "directory": "packages/typed-openapi"
  },
  "scripts": {
    "start": "node ./dist/cli.js",
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "gen:runtime": "node bin.js ./tests/samples/petstore.yaml --output ./tmp/generated-client.ts --tanstack generated-tanstack.ts --default-fetcher",
    "test:runtime:run": "vitest run tests/integration-runtime-msw.test.ts",
    "test:runtime": "pnpm run gen:runtime && pnpm run test:runtime:run",
    "fmt": "prettier --write src",
    "typecheck": "tsc -b ./tsconfig.build.json"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@sinclair/typebox-codegen": "^0.11.1",
    "arktype": "2.1.20",
    "cac": "^6.7.14",
    "openapi3-ts": "^4.4.0",
    "pastable": "^2.2.1",
    "pathe": "^2.0.3",
    "prettier": "3.5.3",
    "ts-pattern": "^5.7.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@tanstack/react-query": "5.85.0",
    "@types/node": "^22.15.17",
    "@types/prettier": "3.0.0",
    "msw": "2.10.5",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "zod": "3.21.4"
  },
  "files": [
    "src",
    "dist",
    "cli",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "openapi",
    "generator",
    "runtime",
    "typesafe",
    "zod",
    "arktype",
    "typebox",
    "valibot",
    "yup",
    "io-ts"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
