{
  "name": "typed-openapi",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/astahmer/typed-openapi.git"
  },
  "scripts": {
    "build": "cd packages/typed-openapi && pnpm build",
    "build:all": "pnpm -r run build",
    "release": "changeset publish",
    "release-dev": "changeset version --snapshot dev && changeset publish --tag dev",
    "test": "cd packages/typed-openapi && pnpm run test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@jsdevtools/ono@7.1.3": "patches/@jsdevtools__ono@7.1.3.patch"
    }
  }
}
